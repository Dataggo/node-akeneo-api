{
  "name": "akeneo-api",
  "version": "1.0.0",
  "description": "A Node Rest Client for the Akeneo PIM",
  "author": "Jens Verbeken",
  "license": "MIT",
  "main": "dist/esm/lib/index.ts",
  "types": "dist/esm/lib/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js"
  },
  "engines": {
    "node": ">=15"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "watch": "tsc  -w -p tsconfig.json && tsc -w -p tsconfig-cjs.json",
    "build:docs": "typedoc"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "qs": "^6.9.6",
    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "@types/qs": "^6.9.5",
    "@types/ramda": "^0.27.36",
    "typedoc": "^0.20.19",
    "typescript": "^4.1.3"
  }
}
